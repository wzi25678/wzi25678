import java.util.Scanner;
 
/**
 * 他计划周一至周五每天做 a 道题目，周六和周日每天做 b 道题目。请你帮小明计算，按照计划他将在第几天实现做题数大于等于 n 题？
 * 输入一行包含三个整数 a, b 和 n.
 * 
 * 
 * @author Wzi
 *
 */
public class Main {
    private static long a,b,n ;
     
    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
         a = sc.nextLong();
         b = sc.nextLong();
         n = sc.nextLong();
         int day = 0;
         int xinqi = 0;
         while(n>0) {
             if(xinqi <5) {
                 n-=(a);
                 xinqi++;
                 day++;
             }
             else {
                 n-=b;
                 xinqi%=7;
                 day++;
             }
             
         }
         System.out.println(day);
    }
 
}
