package Year_2019;

import java.util.Scanner;

public class 糖果 {
	private static int n,m,k;	
	private static int dp[];	//每包糖去dfs
	private static int vistied[];   //标记糖的读取状态
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		n=sc.nextInt();
		m=sc.nextInt();
		k=sc.nextInt();
		dp = new int [(1<<m)];	//m的两倍
		for(int i=0;i<n;i++) {
			for(int j=0;j<k;j++) {
				int teast = sc.nextInt();
				vistied[i] |= 1<<teast -1;
			}
		}
		
	}//end -main
	
}//end -class 糖果
