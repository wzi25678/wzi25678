nn_index = np.argsort( dist)
print("dist: ",dist)
print("---------------------------------------\n")
print("nn_index: ",nn_index)
print("---------------------------------------\n")
nn_y = y_train[nn_index[:9]].ravel()
print(y_train[:8])
print("nn_y: ",nn_y)
print("---------------------------------------\n")
print(np.bincount(nn_y))
print(np.argmax(np.bincount(nn_y)))
